[user]
        email = vadym.balaban@lookout.com
        name = Vadym Balaban
[alias]
        co = checkout
        ci = commit -v
        st = status
        cat = cat-file
        br = branch
        ffwd = pull --rebase
        changes = diff --name-status -r
        diffstat = diff --stat -r
        update = pull --rebase upstream master
        aliases = config --get-regexp alias
        lg = log --graph --decorate --oneline --all
        plg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
        pg = push gerrit HEAD:refs/publish/master
        up = pull --rebase upstream master
[color]
        branch = auto
        diff = auto
        grep = auto
        interactive = auto
        ui = auto
        status = auto
        showbranch = auto
[core]
        excludesfile = /Users/vbalaban/.gitignore_global
        editor = /usr/local/bin/vim
[difftool "sourcetree"]
	cmd = /opt/homebrew-cask/Caskroom/diffmerge/4.2.0.697/DiffMerge.app/Contents/MacOS/DiffMerge --nosplash \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /opt/homebrew-cask/Caskroom/diffmerge/4.2.0.697/DiffMerge.app/Contents/MacOS/DiffMerge --merge --result=\"$MERGED\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"
	trustExitCode = true
